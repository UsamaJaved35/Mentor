        <!-- Favicons -->
        <link href="~/src/mentor.png" rel="icon">
        <link href="~/src/mentor.png" rel="apple-touch-icon">
    </head>
    <body>
              <!-- Modal -->
        <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLongTitle">LOGIN AS A</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body d-flex justify-content-center" id="options" >
                        <a type="button" asp-controller="login" asp-action="logintutor" class="btn btn-success">TEACHER</a>
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        <a type="button" asp-controller="login" asp-action="loginstudent" class="btn btn-outline-success ms-1">STUDENT</a>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- ======= Header ======= -->
        <header id="header" class="fixed-top">
            <div class="container d-flex align-items-center">

                <h1 class="logo me-auto"><a asp-controller="home" asp-action="index" style="text-decoration:none">Mentor</a></h1>
                <!-- Uncomment below if you prefer to use an image logo -->
                <!--<a href="index.html" class="logo me-auto">-->@*<img src="~/src/testimonials/" alt="" class="img-fluid">*@<!--</a>-->

                <nav id="navbar" class="navbar order-last order-lg-0">
                    <ul>
                        <li><a class="active" asp-controller="home" asp-action="Index" style="text-decoration:none">Home</a></li>
                        <li><a asp-controller="about" asp-action="About" style="text-decoration:none">About</a></li>
                        <li class="dropdown" id="login">
                            <a><span>Login</span> <i class="fa fa-chevron-down"></i></a>
                            <ul>
                                <li><a asp-controller="login" asp-action="logintutor" style="text-decoration:none">AS A TEACHER</a></li>
                                <li><a asp-controller="login" asp-action="loginstudent" style="text-decoration:none">AS A STUDENT</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a><span>Sign Up</span> <i class="fa fa-chevron-down"></i></a>
                            <ul>
                                <li><a asp-controller="Tutor" asp-action="tutorform" style="text-decoration:none">AS A TEACHER</a></li>
                                <li><a asp-controller="student" asp-action="studentform" style="text-decoration:none">AS A STUDENT</a></li>
                            </ul>
                        </li>
                        <li><a asp-controller="contact" asp-action="contact">Contact Us</a></li>
                    </ul>
                </nav><!-- .navbar -->
                <!-- Button trigger modal -->
                    <a type="button" style="text-decoration:none" class="get-started-btn" 
                       data-toggle="modal" data-target="#exampleModalCenter">Get Started</a> &nbsp;&nbsp;
                        <a type="button"  onclick="logout()"  id="logout" style="display:none" class="btn btn-outline-success rounded-pill">Logout</a>
                   </div>
        </header><!-- End Header -->
        <div>
            @RenderBody()
        </div>
        <div>
            @if(IsSectionDefined("studentForm")){
            @RenderSection("studentForm")}
            @if(IsSectionDefined("tutorForm")){
            @RenderSection("tutorForm")}
        </div>
        <div>
        <footer id="footer">
            <div class="footer-top">
                <div class="container">
                    <div class="row">

                        <div class="col-lg-3 col-md-6 footer-contact">
                            <h3>Mentor</h3>
                            <p>
                                A108 Adam Street <br>
                                New York, NY 535022<br>
                                United States <br><br>
                                <strong>Phone:</strong> +1 5589 55488 55<br>
                                <strong>Email:</strong> info@example.com<br>
                            </p>
                        </div>

                        <div class="col-lg-2 col-md-6 footer-links">
                            <h4>Useful Links</h4>
                            <ul>
                                <li><i class="fa fa-chevron-right"></i> <a asp-controller="home" asp-action="Index">Home</a></li>
                                <li><i class="fa fa-chevron-right"></i> <a asp-controller="about" asp-action="about">About us</a></li>
                                <li><i class="fa fa-chevron-right"></i> <a asp-controller="login" asp-action="login">Login</a></li>
                                <li><i class="fa fa-chevron-right"></i> <a asp-controller="contact" asp-action="contact">Contact Us</a></li>
                                <li><i class="fa fa-chevron-right"></i> <a href="#">Privacy policy</a></li>
                            </ul>
                        </div>
                        <div class="col-lg-3 col-md-6 footer-links">
                            <h4>Subjects Specialists</h4>
                            <ul>
                                <li><i class="fa fa-chevron-right"></i> Mathematics</li>
                                <li><i class="fa fa-chevron-right"></i> Physics</li>
                                <li><i class="fa fa-chevron-right"></i> Chemistry</li>
                                <li><i class="fa fa-chevron-right"></i>Computer Science</li>
                                <li><i class="fa fa-chevron-right"></i> Biology</li>
                            </ul>
                        </div>

                        <div class="col-lg-4 col-md-6 footer-newsletter">
                            <h4>Join Us</h4>
                            <p>We need a team of tutors with full potential to deliever knowledge to the students! </p>
                            <form action="" method="post">
                                <input type="email" name="email"><input type="submit" value="Subscribe">
                            </form>
                        </div>
                    </div>
                </div>
            </div>

            <div class="container d-md-flex py-4">

                <div class="me-md-auto text-center text-md-start">
                    <div class="copyright">
                        &copy; Copyright <strong><span>Mentor</span></strong>. All Rights Reserved
                    </div>
                </div>
                <div class="social-links text-center text-md-right pt-3 pt-md-0">
                    <a href="#" class="twitter"><i class="fa fa-twitter"></i></a>
                    <a href="#" class="facebook"><i class="fa fa-facebook"></i></a>
                    <a href="#" class="instagram"><i class="fa fa-instagram"></i></a>
                    <a href="#" class="google-plus"><i class="fa fa-skype"></i></a>
                    <a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a>
                </div>
            </div>
        </footer><!-- End Footer -->
        </div>
        <script src="~/js/purecounter.js"></script>
        <script src="~/js/aos.js"></script>
        <script src="~/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/swiper-bundle.min.js"></script>
        <script src="~/js/validate.js"></script>

        <!-- Template Main JS File -->
